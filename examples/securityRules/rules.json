{
  "rules": {
    "myGeoFire": {
      ".read": true,
      ".write": true,
      "$key": {
        ".validate": "newData.child('g').val() != null && newData.child('l').val() != null && newData.getPriority().length <= 22 && newData.getPriority().length > 0",
        "g": {
          ".validate": "newData.isString() && newData.val().length <= 22 && newData.val().length > 0"
        },
        "l": {
          "0" : {
            ".validate": "newData.isNumber() && newData.val() >= -90 && newData.val() <= 90"
          },
          "1" : {
            ".validate": "newData.isNumber() && newData.val() >= -180 && newData.val() <= 180"
          },
          "$other": {
            ".validate": false
          }
        },
        "$other": {
          ".validate": false
        }
      }
    }
  }
}
