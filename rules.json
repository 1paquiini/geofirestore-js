{
  "rules": {
    "i": {
      ".read": true,
      "$geohashkey": {
        //".write": "$geohashkey.isString() && $geohashkey.endsWith(newData.val()) && newData.parent().parent().child('l').child(newData.val()).exists()"
        ".write": "newData.val() == null || newData.val() == true"
      }
    },
    "l": {
      "$key": {
        ".read": true,
        ".write": "newData.val() == null || (newData.isString() && newData.val().contains(','))"
      }
    }
  }
}